{
  "name": "serverless-js",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "serverless": "sls",
    "serverless:offline": "export IS_OFFLINE=true && sls offline start",
    "serverless:package": "serverless webpack",
    "serverless:dynamodb": "sls dynamodb install",
    "delete": "aws stepfunctions --endpoint-url http://localhost:8083 delete-state-machine --state-machine-arn 'arn:aws:states:eu-west-1:803037826383:stateMachine:WaitMachine' &",
    "deploy:dev": "serverless deploy -s development",
    "deploy:remove": "serverless remove",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1129.0",
    "uuid": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.4",
    "@types/aws-lambda": "^8.10.95",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^17.0.31",
    "@types/serverless": "^3.12.4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "aws-ses-v2-local": "^1.2.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "jest": "^28.0.3",
    "prettier": "^2.6.2",
    "serverless": "^3.16.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.7.0",
    "serverless-offline-lambda": "^1.0.6",
    "serverless-offline-ses-v2": "^1.0.0",
    "serverless-offline-sns": "^0.76.0",
    "serverless-offline-sqs": "^6.0.0",
    "serverless-plugin-typescript": "^2.1.2",
    "serverless-step-functions": "^3.7.0",
    "serverless-step-functions-local": "^0.3.0",
    "serverless-webpack": "^5.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
